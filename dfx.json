{
  "canisters": {
    "backend": {
      "main": "backend/Main.mo",
      "type": "motoko"
    },
    "react_app": {
      "dependencies": ["backend"],
      "frontend": {
        "entrypoint": "public/index.html"
      },
      "source": ["build"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "output": "build",
      "packtool": "vessel sources"
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    }
  }
}
