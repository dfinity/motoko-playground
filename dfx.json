{
  "canisters": {
    "didjs": {
      "type": "custom",
      "candid": "src/didjs.did",
      "wasm": "src/didjs.wasm",
      "build": ""
    },
    "playground_assets": {
      "dependencies": ["didjs"],
      "frontend": {
        "entrypoint": "src/index.js"
      },
      "source": [
        "src/assets/",
        "dist/playground_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
